@model IEnumerable<CarRentalManagementSystem.Models.User>

@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Manage Users";
}

<h2>Manage Users</h2>

<table class="table table-striped table-bordered">
    <thead class="table-dark">
        <tr>
            <th>User ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Role</th>
          
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserID</td>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Role</td>
               
            </tr>
        }
    </tbody>
</table>
